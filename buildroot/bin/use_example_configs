#!/usr/bin/env bash

restore_configs

[ -f "Marlin/src/config/examples/Configuration.h" ]     && cp "Marlin/src/config/examples/Configuration.h" Marlin/
[ -f "Marlin/src/config/examples/Configuration_adv.h" ] && cp "Marlin/src/config/examples/Configuration_adv.h" Marlin/
[ -f "Marlin/src/config/examples/_Bootscreen.h" ]       && cp "Marlin/src/config/examples/_Bootscreen.h" Marlin/
[ -f "Marlin/src/config/examples/_Statusscreen.h" ]     && cp "Marlin/src/config/examples/_Statusscreen.h" Marlin/
